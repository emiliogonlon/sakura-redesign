<style>
  #logo {
    display: grid;
    width: 415px;
    grid-template-columns: 100px auto;
    align-items: center;
    gap: var(--spacing-15);
    margin: 15px;
  }

  #circback {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100px;
    height: 100px;
    background-color: #f8f8f8;
  }

  #circ {
    width: 70%;
    height: 70%;
    border-radius: 50%;
    background: conic-gradient(from 225deg at 50% 50%, #ffc0cb 0deg, #ddf5ff 360deg);
  }

  #logodates {
    font-weight: var(--details-desk-font-weight);
    font-size: var(--details-desk-font-size);
    line-height: 0.8em;
    color: #7E7E7E;
  }
</style>
